ALTER TABLE `reservations` ADD `BIKE_ID` INT(10) NOT NULL AFTER `FRAME_NUMBER`;
UPDATE reservations aa set BIKE_ID = (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.FRAME_NUMBER) WHERE exists (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.FRAME_NUMBER);
ALTER TABLE `feedbacks` ADD `BIKE_ID` INT(10) NOT NULL AFTER `BIKE_NUMBER`;
UPDATE feedbacks aa set BIKE_ID = (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.BIKE_NUMBER);
ALTER TABLE customer_bikes DROP PRIMARY KEY;
ALTER TABLE `bike_building_access` ADD `BIKE_ID` INT NOT NULL AFTER `BIKE_NUMBER`;
UPDATE bike_building_access aa set BIKE_ID = (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.BIKE_NUMBER) WHERE exists (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.BIKE_NUMBER);
ALTER TABLE `customer_bike_access` ADD `BIKE_ID` INT(10) NOT NULL AFTER `BIKE_NUMBER`;
UPDATE customer_bike_access aa set BIKE_ID = (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.BIKE_NUMBER) WHERE exists (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.BIKE_NUMBER);
ALTER TABLE `action_log` ADD `BIKE_ID` INT(10) NOT NULL AFTER `BIKE_NUMBER`;
UPDATE action_log aa set BIKE_ID = (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.BIKE_NUMBER) WHERE exists (SELECT ID FROM customer_bikes bb where bb.FRAME_NUMBER=aa.BIKE_NUMBER);
ALTER TABLE `bike_building_access` CHANGE `BIKE_NUMBER` `BIKE_NUMBER` VARCHAR(45) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT NULL;
ALTER TABLE `customer_bike_access` CHANGE `BIKE_NUMBER` `BIKE_NUMBER` VARCHAR(45) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT NULL;
ALTER TABLE `reservations` CHANGE `FRAME_NUMBER` `FRAME_NUMBER` VARCHAR(45) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT NULL;
ALTER TABLE `feedbacks` CHANGE `BIKE_NUMBER` `BIKE_NUMBER` VARCHAR(150) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT NULL;
ALTER TABLE `factures_details` DROP `FRAME_NUMBER`; 
ALTER TABLE `feedbacks` DROP `BIKE_NUMBER`;
ALTER TABLE `locking_bikes` DROP `FRAME_NUMBER`; 
ALTER TABLE `reservations` DROP `FRAME_NUMBER`;
DELETE FROM `customer_bike_access` WHERE BIKE_ID=0;
ALTER TABLE `customer_bike_access` DROP `BIKE_NUMBER`;
DELETE FROM bike_building_access WHERE BIKE_ID=0;
ALTER TABLE `bike_building_access` DROP `BIKE_NUMBER`;
